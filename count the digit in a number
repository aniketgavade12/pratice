import java.util.Scanner;

import java.util.Scanner;

public class countdigit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        int count = 0;

        while (t != 0) {

            t = t/ 10;
            System.out.println(t);
            count++;
        }

        System.out.println("Total count: " + count);
    }
}
